{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],

  "[typescript]": {"editor.formatOnSave": true},
  
  "editor.codeActionsOnSave": {
    "editor.formatOnSave": true,
    "source.fixAll.eslint": true,
    "source.fixAll": true,
    "source.fixAll.prettier": true
  },
}